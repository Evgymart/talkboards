{
    "name": "evgenymart/talkboards",
    "description": "A reddit-like project",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^8.4",
        "baldinof/roadrunner-bundle": "^3.1.3",
        "doctrine/dbal": "^3.9.4",
        "doctrine/doctrine-bundle": "^2.13.2",
        "doctrine/doctrine-migrations-bundle": "^3.4.1",
        "doctrine/orm": "^3.3.1",
        "phpdocumentor/reflection-docblock": "^5.6.1",
        "phpstan/phpdoc-parser": "^2.0",
        "ramsey/uuid": "^4.7.6",
        "symfony/dependency-injection": "~7.1.9",
        "symfony/dotenv": "^7.1.9",
        "symfony/event-dispatcher": "~7.1.6",
        "symfony/flex": "^2.4.7",
        "symfony/framework-bundle": "^7.1.10",
        "symfony/http-foundation": "~7.1.10",
        "symfony/http-kernel": "~7.1.10",
        "symfony/messenger": "~7.1.9",
        "symfony/property-access": "~7.1.6",
        "symfony/property-info": "~7.1.10",
        "symfony/routing": "~7.1.9",
        "symfony/runtime": "^7.1.7",
        "symfony/serializer": "~7.1.9",
        "symfony/validator": "~7.1.10",
        "symfony/yaml": "^7.1.6"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8.2",
        "ergebnis/composer-normalize": "^2.45",
        "friendsofphp/php-cs-fixer": ">=3.66",
        "phpstan/phpstan": "^2.1.2",
        "phpstan/phpstan-phpunit": "^2.0.4",
        "phpunit/phpunit": "^11.5.4",
        "phpyh/coding-standard": "^2.6.2",
        "qossmic/deptrac": "^2.0.4"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "TalkBoards\\": "src/"
        },
        "files": [
            "src/Infrastructure/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TalkBoards\\": [
                "tests/",
                "src-dev/"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    }
}
